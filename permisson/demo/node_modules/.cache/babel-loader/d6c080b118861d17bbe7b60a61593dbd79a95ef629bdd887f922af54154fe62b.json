host:8055').with(rest());\r\n\r\n// 2️⃣ 初始化文件列表和上传状态\r\nconst fileList = ref([]);\r\nlet uploadStatus = ref('');\r\n\r\n// 3️⃣ 文件上传成功/失败时的处理\r\nconst handleChange = (info) => {\r\n  const status = info.file.status;\r\n  if (status !== 'uploading') {\r\n    console.log(info.file, info.fileList);\r\n  }\r\n  if (status === 'done') {\r\n    message.success(`${info.file.name} file uploaded successfully.`);\r\n  } else if (status === 'error') {\r\n    message.error(`${info.file.name} file upload failed.`);\r\n  }\r\n};\r\n\r\n// 4️⃣ 处理文件拖拽事件\r\nfunction handleDrop(e) {\r\n  // 获取拖拽的文件\r\n  const files = e.dataTransfer.files;\r\n  if (files.length > 0) {\r\n    // 使用 FormData 来构建上传的文件数据\r\n    const formData = new FormData();\r\n    for (let file of files) {\r\n      formData.append('file', file);\r\n    }\r\n    console.log(formData);\r\n    // 确保 imageUrl 是 Vue 响应式的 ref 类型\r\nconst imageUrl = ref('');\r\n\r\n// 上传文件处理逻辑\r\nclient.request(uploadFiles(formData))\r\n  .then((response) => {\r\n    console.log('Response:', response);  // 检查 response 数据结构\r\n    \r\n    // 确保 response 是一个数组，并且包含文件信息\r\n    if (response && response.length > 0) {\r\n      const file = response;\r\n      \r\n      // 检查 file 是否有效并且包含 filename_disk\r\n      if (file && file.filename_disk) {\r\n        // 构建文件的访问 URL\r\n        const imageUrlPath = `http://localhost:8055/assets/${file.filename_disk}`;\r\n        \r\n        // 使用 .value 来赋值，因为 imageUrl 是一个 ref 类型\r\n        imageUrl.value = imageUrlPath;\r\n        \r\n        // 更新上传状态\r\n        uploadStatus.value = `Upload successful! File ID: ${file.id}`;\r\n        message.success('File uploaded successfully!');\r\n      } else {\r\n        console.error('File information is missing or invalid.');\r\n      }\r\n    } else {\r\n      console.error('No file returned in response.');\r\n    }\r\n  })\r\n  .catch((error) => {\r\n    // 上传失败处理\r\n    uploadStatus.value = 'Upload failed!';\r\n    message.error('File upload failed!');\r\n    console.error(error);\r\n  });\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.uip {\r\n  height: 200px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAK;;EAUTA,KAAK,EAAC;AAAsB;;EAXrCC,GAAA;EAqByBD,KAAK,EAAC;;mBArB/B;;EAwBOE,EAAE,EAAC;AAAQ;;;uBAvBhBC,mBAAA,CAwBM,OAxBNC,UAwBM,GAvBJC,YAAA,CAiBmBC,2BAAA;IAhBTC,QAAQ,EAAEC,MAAA,CAAAD,QAAQ;IAHhC,qBAAAE,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAGwBF,MAAA,CAAAD,QAAQ,GAAAG,MAAA;IAC1BC,IAAI,EAAC,MAAM;IACXC,MAAM,EAAC,QAAQ;IACdC,QAAQ,EAAE,IAAI;IACdC,QAAM,EAAEN,MAAA,CAAAO,YAAY;IACpBC,MAAI,EAAER,MAAA,CAAAS,UAAU;IAChBC,KAAK,EAAE;MAAAC,KAAA;MAAAC,MAAA;IAAA;;IATdC,OAAA,EAAAC,QAAA,CAWM,MAEI,CAFJC,mBAAA,CAEI,KAFJC,UAEI,GADFnB,YAAA,CAA+BG,MAAA,mB,6BAEjCe,mBAAA,CAAwE;MAArEvB,KAAK,EAAC;IAAiB,GAAC,2CAAyC,sB,0BACpEuB,mBAAA,CAGI;MAHDvB,KAAK,EAAC;IAAiB,GAAC,0GAG3B,qB;IAlBNyB,CAAA;mCAoBIC,mBAAA,kBAAqB,EACVC,IAAA,CAAAC,QAAQ,I,cAAnBzB,mBAAA,CAEM,OAFN0B,UAEM,GADJN,mBAAA,CAAwF;IAAlFO,GAAG,EAAEH,IAAA,CAAAC,QAAQ;IAAEG,GAAG,EAAC,gBAAgB;IAACb,KAA2C,EAA3C;MAAA;MAAA;IAAA;0BAtBhDc,UAAA,E,KAAAN,mBAAA,gBAwBIH,mBAAA,CAAqC,KAArCU,UAAqC,EAAAC,gBAAA,CAAnB1B,MAAA,CAAA2B,YAAY,iB","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}