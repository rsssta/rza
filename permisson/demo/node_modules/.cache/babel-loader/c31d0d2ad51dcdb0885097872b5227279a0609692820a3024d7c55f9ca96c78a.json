/ 文件上传状态处理\r\nconst handleChange = (info) => {\r\n  const status = info.file.status;\r\n  if (status !== 'uploading') {\r\n    console.log(info.file, info.fileList);\r\n  }\r\n  if (status === 'done') {\r\n    message.success(`${info.file.name} file uploaded successfully.`);\r\n  } else if (status === 'error') {\r\n    message.error(`${info.file.name} file upload failed.`);\r\n  }\r\n};\r\n\r\n// 处理文件拖拽上传\r\nconst handleDrop = async (e) => {\r\n  const files = e.dataTransfer.files;\r\n  if (files.length === 0) return;\r\n\r\n  const formData = new FormData();\r\n  for (let file of files) {\r\n    formData.append('file', file);\r\n  }\r\n\r\n  try {\r\n    console.log('Uploading file:', formData);\r\n    const response = await client.request(uploadFiles(formData));\r\n\r\n    console.log('Upload Response:', response);\r\n    if (!response || !response.id) {\r\n      console.error('Invalid response:', response);\r\n      message.error('Upload failed: No valid file ID received.');\r\n      return;\r\n    }\r\n\r\n    // 生成 Directus 资源 URL\r\n    imageUrl.value = `http://localhost:8055/assets/${response.id}`;\r\n    console.log('Image URL:', imageUrl.value);\r\n    message.success('File uploaded successfully!');\r\n\r\n  } catch (error) {\r\n    console.error('Upload failed:', error);\r\n    message.error('File upload failed!');\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n/* 让整体布局居中并垂直排列 */\r\n.uip {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  height: 100vh;\r\n}\r\n\r\n/* 让上传的图片居中 */\r\n.image-container {\r\n  margin-top: 20px;\r\n}\r\n\r\n.uploaded-image {\r\n  width: 200px;\r\n  max-height: 200px;\r\n  border-radius: 10px;\r\n  box-shadow: 2px 2px 10px rgba(0, 0, 0, 0.2);\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAK;;EAUTA,KAAK,EAAC;AAAsB;;EAXrCC,GAAA;EAsByBD,KAAK,EAAC;;mBAtB/B;;EA0BOE,EAAE,EAAC;AAAQ;;;uBAzBhBC,mBAAA,CA0BM,OA1BNC,UA0BM,GAzBJC,mBAAA,WAAc,EACdC,YAAA,CAgBmBC,2BAAA;IAfTC,QAAQ,