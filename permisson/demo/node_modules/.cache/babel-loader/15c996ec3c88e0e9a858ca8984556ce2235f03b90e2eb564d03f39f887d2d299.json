{"ast":null,"code":"import { reactive, h, ref, onMounted, watch } from 'vue';\nimport { RouterLink } from 'vue-router';\nimport { PieChartOutlined, MailOutlined, MenuFoldOutlined, MenuUnfoldOutlined } from '@ant-design/icons-vue';\n// 左侧菜单和折叠状态\n\nexport default {\n  __name: 'Layout',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const state = reactive({\n      collapsed: false,\n      selectedKeys: ['1'],\n      openKeys: ['sub1'],\n      preOpenKeys: ['sub1']\n    });\n    // 定义菜单项\n    const items = reactive([{\n      key: 'sub1',\n      icon: () => h(MailOutlined),\n      label: '产品管理',\n      children: [{\n        key: '1',\n        label: '产品信息',\n        route: '/productInfo' // 设置路由\n      }, {\n        key: '2',\n        label: 'Option 6',\n        route: '/option-6' // 设置路由\n      }]\n    }, {\n      key: '3',\n      icon: () => h(PieChartOutlined),\n      label: '文字提取',\n      route: '/textExtract' // 设置路由\n    }]);\n    // 动态展开的菜单\n    watch(() => state.openKeys, (_val, oldVal) => {\n      state.preOpenKeys = oldVal;\n    });\n    // 处理折叠菜单\n    const toggleCollapsed = () => {\n      state.collapsed = !state.collapsed;\n      state.openKeys = state.collapsed ? [] : state.preOpenKeys;\n    };\n    const __returned__ = {\n      state,\n      items,\n      toggleCollapsed,\n      reactive,\n      h,\n      ref,\n      onMounted,\n      watch,\n      get RouterLink() {\n        return RouterLink;\n      },\n      get PieChartOutlined() {\n        return PieChartOutlined;\n      },\n      get MailOutlined() {\n        return MailOutlined;\n      },\n      get MenuFoldOutlined() {\n        return MenuFoldOutlined;\n      },\n      get MenuUnfoldOutlined() {\n        return MenuUnfoldOutlined;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["reactive","h","ref","onMounted","watch","RouterLink","PieChartOutlined","MailOutlined","MenuFoldOutlined","MenuUnfoldOutlined","state","collapsed","selectedKeys","openKeys","preOpenKeys","items","key","icon","label","children","route","_val","oldVal","toggleCollapsed"],"sources":["E:/permisson/demo/src/views/Layout.vue"],"sourcesContent":["<template>\r\n  <a-layout class=\"layout\">\r\n    <!-- 左侧 -->\r\n    <a-layout-sider v-model:collapsed=\"collapsed\" :trigger=\"null\" collapsible>\r\n      <div class=\"logo\" />\r\n      <a-menu v-model:openKeys=\"state.openKeys\" v-model:selectedKeys=\"state.selectedKeys\" mode=\"inline\" theme=\"dark\"\r\n        :inline-collapsed=\"state.collapsed\">\r\n        <template v-for=\"item in items\">\r\n          <!-- 如果有子菜单，使用 a-sub-menu -->\r\n          <template v-if=\"item.children\">\r\n            <a-sub-menu :key=\"`submenu-${item.key}`\" :icon=\"item.icon\" :title=\"item.label\">\r\n              <a-menu-item v-for=\"subItem in item.children\" :key=\"`subitem-${subItem.key}`\">\r\n                <RouterLink :to=\"subItem.route\">{{ subItem.label }}</RouterLink>\r\n              </a-menu-item>\r\n            </a-sub-menu>\r\n          </template>\r\n          <!-- 如果没有子菜单，直接使用 a-menu-item -->\r\n          <template v-else>\r\n            <a-menu-item :key=\"`menuitem-${item.key}`\">\r\n              <RouterLink :to=\"item.route\">\r\n                <span v-if=\"item.icon\">\r\n                  <component :is=\"item.icon\" />\r\n                </span>\r\n                {{ item.label }}\r\n              </RouterLink>\r\n            </a-menu-item>\r\n          </template>\r\n\r\n        </template>\r\n      </a-menu>\r\n    </a-layout-sider>\r\n    <!-- 右侧 -->\r\n    <a-layout>\r\n      <!-- 顶部 -->\r\n      <a-layout-header style=\"background: '#fff'; padding: 0\">\r\n        <menu-unfold-outlined v-if=\"collapsed\" class=\"trigger\" @click=\"() => (collapsed = !collapsed)\" />\r\n        <menu-fold-outlined v-else class=\"trigger\" @click=\"() => (collapsed = !collapsed)\" />\r\n      </a-layout-header>\r\n      <a-layout-content :style=\"{ margin: '6px', padding: '6px', background: '#fff', minHeight: '280px' }\">\r\n        <router-view></router-view>\r\n      </a-layout-content>\r\n    </a-layout>\r\n  </a-layout>\r\n</template>\r\n<script setup>\r\nimport { reactive, h, ref, onMounted, watch } from 'vue';\r\nimport { RouterLink } from 'vue-router';\r\nimport { PieChartOutlined, MailOutlined, MenuFoldOutlined, MenuUnfoldOutlined } from '@ant-design/icons-vue';\r\n// 左侧菜单和折叠状态\r\nconst state = reactive({\r\n  collapsed: false,\r\n  selectedKeys: ['1'],\r\n  openKeys: ['sub1'],\r\n  preOpenKeys: ['sub1'],\r\n});\r\n// 定义菜单项\r\nconst items = reactive([\r\n  {\r\n    key: 'sub1',\r\n    icon: () => h(MailOutlined),\r\n    label: '产品管理',\r\n    children: [\r\n      {\r\n        key: '1',\r\n        label: '产品信息',\r\n        route: '/productInfo',  // 设置路由\r\n      },\r\n      {\r\n        key: '2',\r\n        label: 'Option 6',\r\n        route: '/option-6',  // 设置路由\r\n      },\r\n    ],\r\n  },\r\n  {\r\n    key: '3',\r\n    icon: () => h(PieChartOutlined),\r\n    label: '文字提取',\r\n    route: '/textExtract',  // 设置路由\r\n  },\r\n]);\r\n// 动态展开的菜单\r\nwatch(\r\n  () => state.openKeys,\r\n  (_val, oldVal) => {\r\n    state.preOpenKeys = oldVal;\r\n  }\r\n);\r\n// 处理折叠菜单\r\nconst toggleCollapsed = () => {\r\n  state.collapsed = !state.collapsed;\r\n  state.openKeys = state.collapsed ? [] : state.preOpenKeys;\r\n};\r\n</script>\r\n<style scoped lang=\"scss\">\r\n.layout {\r\n  width: 100vw;\r\n  height: 100vh;\r\n}\r\n.logo {\r\n  height: 32px;\r\n  background: rgba(255, 255, 255, 0.3);\r\n  margin: 16px;\r\n}\r\n.trigger {\r\n  font-size: 18px;\r\n  line-height: 64px;\r\n  padding: 0 24px;\r\n  cursor: pointer;\r\n  transition: color 0.3s;\r\n  color: #fff;\r\n  &:hover {\r\n    color: #1890ff;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AA6CA,SAASA,QAAQ,EAAEC,CAAC,EAAEC,GAAG,EAAEC,SAAS,EAAEC,KAAK,QAAQ,KAAK;AACxD,SAASC,UAAU,QAAQ,YAAY;A